# MongoDB Configuration
# Copy this file to .env and update with your actual credentials

# MongoDB Connection URL - Include authSource=admin for authentication
DB_CONNECTION_URL=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&authSource=admin

# Database Names
DB_NAME_STG=GRUBTECH_MASTER_DATA_STG_V2
DB_NAME_PROD=GRUBTECH_MASTER_DATA_PROD_V2

# Logging Configuration
LOG_LEVEL=INFO